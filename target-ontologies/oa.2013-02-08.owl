        <rdf:type rdf:resource="&skos;ConceptScheme"/>
        <rdfs:label xml:lang="en">motivationScheme</rdfs:label>
        <rdfs:comment xml:lang="en">The concept scheme for Open Annotation Motivations</rdfs:comment>
    </owl:NamedIndividual>
    

    

