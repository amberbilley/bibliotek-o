<?xml version="1.0" encoding="UTF-8"?>

<!-- TODO Add/remove as needed -->
<rdf:RDF  
    xmlns="http://bib.ld4l.org/ontology/"
    xmlns:bf="http://id.loc.gov/ontologies/bibframe/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:ld4l="http://bib.ld4l.org/ontology/"
    xmlns:oa="http://www.w3.org/ns/oa#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"  
    xmlns:schema="http://schema.org/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    >
    
    <owl:Ontology rdf:about="http://bib.ld4l.org/ontology/">
        <owl:ontologyIRI rdf:resource="http://bib.ld4l.org/ontology"/>
        <!-- TODO Need redirect from the version IRI to the ontology IRI -->
        <!-- TODO Fill in date of publication -->       
        <owl:versionIRI rdf:resource="http://bib.ld4l.org/2017/02/10/ontology"/>
        <!-- TODO Fill in datetime of publication -->
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2017-02-10T15:31:13.103-04:00</owl:versionInfo>
        <!-- TODO Fill in date of publication -->
        <dcterms:issued>2017-02-10</dcterms:issued>
        <dcterms:modified>2016-02-10</dcterms:modified>
        <vann:preferredNamespacePrefix>ld4l</vann:preferredNamespacePrefix>
    </owl:Ontology>
    
    <!-- CLASSES -->
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/TitleElement">
        <rdfs:label>Title element</rdfs:label>    
        <skos:definition>A part of a title, such as a subtitle, part number, or part name.</skos:definition>
    </owl:Class>    
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/MainTitleElement">
        <rdfs:label>Main title element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The main part of a title.</skos:definition>
    </owl:Class>    
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NonSortElement">
        <rdfs:label>Non-sort element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The initial segment of a title that is removed for sorting.</skos:definition>
    </owl:Class>       
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PartNameElement">
        <rdfs:label>Part name element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The part of a title that specifies the name of a part of the resource.</skos:definition>
    </owl:Class>       
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PartNumberElement">
        <rdfs:label>Part number element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The part of a title that specifies the number of a part of the resource.</skos:definition>
    </owl:Class>      
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/SubtitleElement">
        <rdfs:label>Subtitle element</rdfs:label>   
        <rdfs:subClassOf rdf:resource="http://bib.ld4l.org/ontology/TitleElement"/>
        <skos:definition>The subtitle of a title.</skos:definition>
    </owl:Class>       
    
    <!--
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/GeographicLocation">
        <rdfs:label>Geographic location</rdfs:label>
        #TBD Taken from prov-o definition of prov:Location. Is ISO 19112 just an example?#
        <skos:definition>An identifiable geographic place (ISO 19112)</skos:definition>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Location"/>      
    </owl:Class>
    -->

    <!-- Or <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Origin"> -->
    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/TitleOrigin">
        <rdfs:label>Title origin</rdfs:label>    
        <skos:definition>The place from which a title originates, such as spine, cover, container, etc.</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Software">
        <rdfs:label>Software</rdfs:label>    
        <skos:definition>A compiled executable binary file</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/Activity">
        <rdfs:label>Activity</rdfs:label>    
        <skos:definition>An activity or contribution by a single agent that affects or alters the existence or state of a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    
    <!-- Preliminary set of subclasses: will add more -->  
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/AuthorActivity">
        <rdfs:label>Author</rdfs:label>    
        <skos:definition>The activity of authoring a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/IllustratorActivity">
        <rdfs:label>Illustrator</rdfs:label>    
        <skos:definition>The activity of illustrating a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/EditorActivity">
        <rdfs:label>Editor</rdfs:label>    
        <skos:definition>The activity of editing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/CreatorActivity">
        <rdfs:label>Creator</rdfs:label>    
        <skos:definition>The activity of creating a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ComposerActivity">
        <rdfs:label>Composer</rdfs:label>    
        <skos:definition>The activity of composing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/TranslatorActivity">
        <rdfs:label>Translator</rdfs:label>    
        <skos:definition>The activity of translatin a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ConductorActivity">
        <rdfs:label>Conductor</rdfs:label>    
        <skos:definition>The activity of conducting a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/NarratorActivity">
        <rdfs:label>Narrator</rdfs:label>    
        <skos:definition>The activity of narrating a  resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PerformerActivity">
        <rdfs:label>Performer</rdfs:label>    
        <skos:definition>The activity of performing in a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/DistributorActivity">
        <rdfs:label>Distributor</rdfs:label>    
        <skos:definition>The activity of distributing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ManufacturerActivity">
        <rdfs:label>Manufacturer</rdfs:label>    
        <skos:definition>The activity of manufacturing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/PublisherActivity">
        <rdfs:label>Publishing</rdfs:label>    
        <skos:definition>The activity of publishing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://bib.ld4l.org/ontology/ProducerActivity">
        <rdfs:label>Producer</rdfs:label>    
        <skos:definition>The activity of preducing a resource.</skos:definition>
        <rdfs:subClassOf rdfs:resource="http://bib.ld4l.org/ontology/Activity"/>
    </owl:Class>

    
    <!-- OBJECT PROPERTIES -->
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/atLocation">
        <rdfs:label>at location</rdfs:label>
        <skos:definition>Location of any resource</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/> 
    </owl:ObjectProperty>  
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/origin">
        <rdfs:label>Origin</rdfs:label>    
        <skos:definition>Specifies the place from which a title originates, such as spine, cover, container, etc. </skos:definition>
        <rdfs:comment>Distinct from bf:source, which specifies an external organization or scheme from which the title was obtained.</rdfs:comment>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Title"/>
        <!-- OR <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/Origin"/> -->
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/TitleOrigin"/>
        <owl:inverseOf rdfs:resource="http://id.loc.gov/ontologies/bibframe/identifiedBy"/> 
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasActivity">
        <rdfs:label>has activity</rdfs:label>
        <skos:definition>Relates this resource to an activity or contribution by a single agent that affects or alters its existence or state.</skos:definition>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isActivityOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/isActivityOf">
        <rdfs:label>has activity</rdfs:label>
        <skos:definition>Relates the activity being described to the affected resource.</skos:definition>
        <rdfs:domain rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/hasActivity"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/hasAgent">
        <rdfs:label>has agent</rdfs:label>
        <skos:definition>The activity being described is associated with the specified agent.</skos:definition>
        <rdfs:domain rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/isAgentOf"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/isAgentOf">
        <rdfs:label>is agent of</rdfs:label>
        <skos:definition>The specified activity is associated with the agent being described.</skos:definition>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://bib.ld4l.org/ontology/Activity"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/hasAgent"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/atLocation">
        <rdfs:label>at location</rdfs:label>
        <skos:definition>The resource being described is at the specified location.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/> 
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/locationOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://bib.ld4l.org/ontology/locationOf">
        <rdfs:label>location of</rdfs:label>
        <skos:definition>The specified resource is at the location being described.</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/>
        <owl:inverseOf rdf:resource="http://bib.ld4l.org/ontology/atLocation"/>
    </owl:ObjectProperty>
    
    
    <!-- DATATYPE PROPERTIES -->
    
    
    
    <!-- NAMED INDIVIDUALS -->

    
    <owl:NamedIndividual rdf:about="http://bib.ld4l.org/ontology/reviewing">
        <rdf:type rdf:resource="http://www.w3.org/ns/oa#Motivation"/>
        <rdfs:label xml:lang="en-us">reviewing</rdfs:label>
        <rdfs:comment xml:lang="en-us">The motivation that represents a reviewing  of the target resource.</rdfs:comment>
        <skos:inScheme rdf:resource="http://www.w3.org/ns/oa#motivationScheme"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://bib.ld4l.org/ontology/cataloging">
        <rdf:type rdf:resource="http://www.w3.org/ns/oa#Motivation"/>
        <rdfs:label xml:lang="en-us">cataloging</rdfs:label>
        <skos:definition xml:lang="en-us">The motivation that represents cataloging or recording metadata.</skos:definition>
        <rdfs:comment xml:lang="en-us">The motivation that represents cataloging or recording metadata.</rdfs:comment>
        <skos:inScheme rdf:resource="http://www.w3.org/ns/oa#motivationScheme"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://bib.ld4l.org/ontology/summarizing">
        <rdf:type rdf:resource="http://www.w3.org/ns/oa#Motivation"/>
        <rdfs:label xml:lang="en-us">summarizing</rdfs:label>
        <rdfs:comment xml:lang="en-us">The motivation that represents a summarizing of the target resource.</rdfs:comment>
        <skos:inScheme rdf:resource="http://www.w3.org/ns/oa#motivationScheme"/>
    </owl:NamedIndividual>

    
</rdf:RDF>


